{
    "name": "brand-snapshot",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "dev": "next dev",
        "dev:preview": "next dev -p 3010",
        "build": "next build --webpack",
        "start": "next start",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "lint": "eslint . --ext .ts,.tsx",
        "types:supabase": "npx supabase gen types typescript --project-id wunderbar-digital-prod > supabase/types.ts",
        "scan": "npx tsx scripts/health-check.ts",
        "scan:fix": "npx tsx scripts/health-check.ts --fix",
        "scan:full": "npx tsx scripts/health-check.ts --build",
        "lint:fix": "eslint . --ext .ts,.tsx --fix",
        "typecheck": "tsc --noEmit --pretty",
        "audit": "npm audit --production",
        "audit:fix": "npm audit fix --production",
        "health": "curl -s http://localhost:3000/api/health | node -e \"process.stdin.on('data',d=>{const h=JSON.parse(d);console.log(h.status==='healthy'?'✅ Healthy':'⚠️ '+h.status);Object.entries(h.checks).forEach(([k,v])=>console.log('  '+k+': '+(v.ok?'✅':'❌ '+v.error)))})\"",
        "setup:ac": "npx tsx scripts/setup-activecampaign.ts"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.74.0",
        "@google/generative-ai": "^0.24.1",
        "@react-pdf/renderer": "^4.3.1",
        "@sentry/nextjs": "^10.38.0",
        "@types/uuid": "^10.0.0",
        "@vercel/og": "^0.8.5",
        "disposable-email-domains": "^1.0.62",
        "next": "^16.1.6",
        "openai": "^6.9.1",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "sharp": "^0.34.5",
        "stripe": "^17.0.0",
        "uuid": "^13.0.0",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.3",
        "@next/bundle-analyzer": "^16.1.6",
        "@supabase/ssr": "^0.8.0",
        "@supabase/supabase-js": "^2.87.1",
        "@types/node": "^20",
        "@types/react": "^19.2.10",
        "@types/react-dom": "^19.2.3",
        "@vitest/ui": "^4.0.18",
        "autoprefixer": "^10.4.22",
        "eslint": "^9.39.2",
        "eslint-config-next": "^16.1.6",
        "postcss": "^8.5.6",
        "supabase": "^2.65.8",
        "tailwindcss": "^4.2.0",
        "typescript": "^5.5.3",
        "vitest": "^4.0.18"
    }
}
