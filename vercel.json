{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "app.brandsnapshot.ai"
        }
      ],
      "destination": "https://app.wunderbrand.ai/$1",
      "permanent": true
    }
  ],
  "crons": [
    {
      "path": "/api/cron/health-check",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/refresh-reminders",
      "schedule": "0 10 * * *"
    }
  ]
}
